<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OA.WindowApp.Converters.EmptyControl">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
        <Image Source="{SvgImage /Assets/empty.svg}" Stretch="UniformToFill" Height="250" Width="500"/>
        <TextBlock Name="Block" Classes="h2" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Classes="TransparentButton" HorizontalAlignment="Center" VerticalAlignment="Center" Click="BaskClick" Content="回到主页"/>
    </StackPanel>
</UserControl>
